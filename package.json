{
  "name": "busiman",
  "version": "0.0.1",
  "description": "A business organizer application",
  "main": "./src/main/main.js",
  "scripts": {
    "start3": "./node_modules/.bin/electron ./dist/main.bundle.js",
    "electron": "electron ./dist/main.bundle.js",
    "start4": "nodemon ./src/main.js --exec \"yarn run electron\"",
    "start": "nodemon --watch ./src/renderer/ && yarn run electron",
    "development": "rimraf dist && webpack --watch --config ./webpack.dev.js --progress --colors",
    "dev": "webpack --config ./webpack.dev.js --progress --colors",
    "dev-run": "yarn run dev && yarn run electron",
    "production": "rimraf dist && webpack --config ./webpack.prod.js --progress --colors",
    "bundle": "webpack --watch --config ./webpack.dev.js --progress --colors --mode development",
    "serve": "electron ./dist/main.bundle.js",
    "start2": "npm-run-all --parallel bundle serve"
  },
  "author": "Christos Lytras <christos.lytras@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "css-loader": "^1.0.1",
    "electron": "3.0.10",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.10.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.26.1",
    "webpack-cli": "^3.1.2"
  },
  "dependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.8",
    "@fortawesome/free-solid-svg-icons": "^5.5.0",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "aphrodite": "^2.2.3",
    "app-root-path": "^2.1.0",
    "babel-loader": "^8.0.4",
    "bootstrap": "^4.1.3",
    "electron-compile": "^6.4.3",
    "electron-hot-loader": "^0.8.0",
    "electron-reload": "^1.3.0",
    "glob": "^7.1.3",
    "glob-fs": "^0.1.7",
    "nodemon": "^1.18.9",
    "platform": "^1.3.5",
    "react": "^16.6.3",
    "react-accessible-accordion": "^2.4.5",
    "react-dom": "^16.6.3",
    "react-localization": "^1.0.13",
    "react-new-window": "^0.0.9",
    "react-popper": "^1.3.0",
    "react-router-dom": "^4.3.1",
    "react-splitter-layout": "^3.0.1",
    "react-transition-group": "^2.5.0",
    "reactstrap": "^6.5.0",
    "read-data": "^1.1.0"
  }
}
